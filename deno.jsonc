{
  "fmt": {
    "options": {
      "indentWidth": 2,
      "lineWidth": 160
    }
  },
  "tasks": {
    "init": "deno run -A init.ts",
    "pre-commit": "deno fmt && deno lint && deno test && deno doc --json > _doc.json",
    "cache": "deno cache --reload --lock=lock.json --lock-write deps.ts"
  }
}
